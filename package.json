    "babel-core": "^5.8.23",
    "bser": "^1.0.2",
    "chalk": "^1.1.1",
    "connect": "^2.8.3",
    "debug": "^2.2.0",
    "fbjs-scripts": "^0.2.1",
    "graceful-fs": "^4.1.2",
    "image-size": "^0.3.5",
    "joi": "^6.6.1",
    "optimist": "^0.6.1",
    "progress": "^1.1.8",
    "promise": "^7.0.4",
    "regenerator": "^0.8.36",
    "sane": "^1.2.0",
    "semver": "^5.0.1",
    "source-map": "^0.4.4",
    "uglify-js": "^2.4.24",
    "underscore": "^1.8.3",
